acmd("cikl", function (playerid) {
    cikl(playerid, 0)
    //setPlayerAnimStyle(playerid, "common", "WalkTorchCivil");
});

function cikl (playerid, i) {
    log(i.tostring(), anim[i][1]);
    local k = i + 1;
    //setPlayerHandModel(playerid, 1, i);
    setPlayerAnimStyle(playerid, anim[i][0],  anim[i][1]);
    delayedFunction(7000, function () {
        cikl(playerid, k);
    })
}

acmd("hand", function (playerid, id) {
    setPlayerHandModel(playerid, 1, id.tointeger());
})

acmd("handl", function (playerid, id) {
    setPlayerHandModel(playerid, 2, id.tointeger());
})

acmd("pr", function (playerid, id) {
    local k = id.tointeger();
    local item = anim[k][1] ? anim[k][1] : 0;
    local model = anim[k][0] ? anim[k][0] : 10;
    setPlayerModel(playerid, model)
    setPlayerHandModel(playerid, anim[k][2], item);
    setPlayerAnimStyle(playerid, anim[k][3],  anim[k][4]);
})
